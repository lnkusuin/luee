    "local-publish": "npm publish --access public && git push origin master --force",
    "analyze": "webpack"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inkusu/luee"
  },
  "keywords": [
    "typescript",
    "node",
    "vue"
  ],
  "author": {
    "name": "Wakai Yutaka",
    "email": "lnkusuin@gmail.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/inkusu/luee/issues"
  },
  "homepage": "https://github.com/inkusu/luee#readme",
  "peerDependencies": {
    "typescript": ">=2.0"
  },
  "dependencies": {
    "dayjs": "^1.9.4",
    "in-viewport": "^3.6.0",
    "lodash-es": "^4.17.15",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "30.0.0",
    "@types/lodash.get": "4.4.9",
    "@types/lodash.has": "4.5.9",
    "jest": "30.0.5",
    "rimraf": "6.0.1",
    "ts-jest": "29.4.1",
    "tslint": "6.1.3",
    "tslint-config-standard": "9.0.0",
    "typescript": "5.9.2",
    "webpack": "5.101.3",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "6.0.1"
  },
  "publishConfig": {
    "access": "public"

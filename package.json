{
  "name": "luee",
  "version": "0.0.17",
  "description": "Vueの拡張",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "tslint \"src/**/*.ts\" --fix --project tsconfig.json",
    "build": "rm -rf dist && tsc",
    "test-spec": "npx jest --coverage",
    "prepare": "npm run build && npm run lint",
